<Window x:Class="LectureProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LectureProject"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Background="Azure"
        Title="LZSS" Height="100" Width="350">
    <Grid>
        <Button Click="Code_OnClick" 
                Height="30" 
                Width="150"
                VerticalAlignment="Center"
                HorizontalAlignment="Left"
                Margin="10">
            Закадировать файл
        </Button>
        <Button Click="Decode_OnClick" 
                Height="30" 
                Width="150"
                VerticalAlignment="Center"
                HorizontalAlignment="Right"
                Margin="10">
            Раскадировать файл
        </Button>
    </Grid>
</Window>
